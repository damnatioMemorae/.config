{
        "$schema": "https://raw.githubusercontent.com/EmmyLuaLs/emmylua-analyzer-rust/refs/heads/main/crates/emmylua_code_analysis/resources/schema.json",
        "completion": {
                "autoRequire": true,
                "autoRequireFunction": "require",
                "autoRequireNamingConvention": "camel-case",
                "autoRequireSeparator": ".",
                "baseFunctionIncludesName": true,
                "callSnippet": true,
                "enable": true,
                "postfix": "@"
        },
        "signature": {
                "detailSignatureHelper": true
        },
        "diagnostics": {
                "globals": [],
                "severity": {
                        "undefined-global": "warning",
                        "unused-local": "information"
                }
        },
        "hint": {
                "enable": true,
                "enumParamHint": true,
                "indexHint": true,
                "localHint": true,
                "metaCallHint": true,
                "overrideHint": true,
                "paramHint": true
        },
        "runtime": {
                "classDefaultCall": {
                        "forceNonColon": false,
                        "forceReturnSelf": false,
                        "functionName": ""
                },
                "extensions": [],
                "frameworkVersions": [],
                "requireLikeFunction": [],
                "requirePattern": [],
                "version": "LuaJIT"
        },
        "workspace": {
                "enableReindex": true,
                "encoding": "utf-8",
                "ignoreDir": [],
                "ignoreGlobs": [],
                "library": [],
                "moduleMap": [],
                "preloadFileSize": 0,
                "reindexDuration": 5000,
                "workspaceRoots": []
        },
        "strict": {
                "arrayIndex": true,
                "docBaseConstMatchBaseType": true,
                "metaOverrideFileDefine": true,
                "requirePath": true,
                "typeCall": true
        },
        "hover": {
                "enable": true
        },
        "codeLens": {
                "enable": true
        },
        "semanticTokens": {
                "enable": true
        },
        "resource": {
                "paths": []
        },
        "references": {
                "enable": true,
                "fuzzySearch": true,
                "shortStringSearch": false
        },
        "inlineValues": {
                "enable": true
        },
        "documentColor": {
                "enable": true
        },
        "doc": {
                "knownTags": [],
                "privateName": []
        },
        "codeAction": {
                "insertSpace": false
        }
}
