$HOME/.config/waybar/side.jsonc|killall waybar && $HOME/.local/share/bin/waybarswitch ht
{
        "mod": "dock",
        "name": "main",
        "layer": "top",
        "margin": "20 10 20 -10",
        "width": 36,
        "position": "right",
        "fixed-center": true,
        "exclusive": true,
        "passthrough": false,
        "gtk-layer-shell": false,
        "reload_style_on_change": true,
        "modules-left": ["custom/separators", "hyprland/workspaces", ""],
        "modules-center": [
                "cpu",
                "memory",
                "custom/updates",
                ""
        ],
        "modules-right": [
                "custom/separators",
                "battery",
                "custom/separators",
                "group/tools",
                ""
        ],
        "group/tools": {
                "orientation": "vertical",
                "drawer": {
                        "transition-duration": 400,
                        "children-class": "not-power",
                        "transition-left-to-right": false
                },
                "modules": [
                        "custom/power",
                        ""
                ]
        },
        "custom/power": {
                "format": "Û±óº",
                "tooltip": false,
                "menu": "on-click",
                "menu-file": "~/.config/waybar/menu/power.xml",
                "menu-actions": {
                        "shutdown": "shutdown",
                        "reboot": "reboot",
                        "suspend": "systemctl suspend",
                        "hibernate": "systemctl hibernate"
                }
        },
        "custom/updates": {
                "exec": "systemupdate w",
                "return-type": "json",
                "format": "{}",
                "interval": 43200,
                "tooltip": true,
                "signal": 20
        },
        "hyprland/workspaces": {
                "rotate": 0,
                "all-outputs": true,
                "active-only": false,
                "on-click": "activate",
                "disable-scroll": false,
                "on-scroll-down": "hyprctl dispatch workspace -1",
                "on-scroll-up": "hyprctl dispatch workspace +1",
                "format": "{icon}",
                "format-icons": {
