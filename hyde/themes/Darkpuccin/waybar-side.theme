        "hyprland/language": {
                "format": "{}",
                // "format-en": "üÆã",
                // "format-ru": "ü¨ã",
                "format-en": "Ó≠î",
                "format-ru": "Ó≠ë",
                "on-click": "keyboardswitch"
        },
        "cava": {
                "framerate": 120,
                "autosens": 1,
                "bars": 40,
                "method": "pulse",
                "source": "auto",
                "stereo": false,
                "reverse": false,
                "bar_delimiter": 0,
                "monstercat": false,
                "waves": true,
                "noise_reduction": 0.77,
                "input_delay": 0,
                "format-icons": ["‚£ø", "‚£æ", "‚£Ω", "‚£ª", "‚¢ø", "‚°ø", "‚£ü", "‚£ø"],
                "actions": {
                        "on-click-right": "mode"
                }
        },
        "backlight": {
                "states": {
                        "min": 0,
                        "midl": 25,
                        "mid": 50,
                        "midh": 75,
                        "max": 100
                },
                "device": "intel_backlight",
                "format": "{icon}",
                "on-click": "$HOME/.local/share/bin/hyprsunset",
                "on-click-right": "shaders",
                "tooltip-format": "{icon} {percent}",
                "format-icons": ["‚°Ä", "‚£Ä", "‚£Ñ", "‚£§", "‚£¶", "‚£∂", "‚£∑", "‚£ø"],
                "on-scroll-up": "brightnessctl set 1%+",
                "on-scroll-down": "brightnessctl set 1%-",
                "min-length": 1
        },
        "pulseaudio": {
                "states": {
                        "min": 0,
                        "midl": 25,
                        "mid": 50,
                        "midh": 75,
                        "max": 100
                },
                "format": "{icon}",
                "format-muted": "Û∞ùü",
                "format-icons": {
                        "headphone": "Û∞ãã",
                        "hands-free": "Û∞•∞",
                        "headset": "Û∞•∞",
                        "default": ["‚°Ä", "‚£Ä", "‚£Ñ", "‚£§", "‚£¶", "‚£∂", "‚£∑", "‚£ø"]
                },
                "tooltip": true,
                "tooltip-format": "Device: {desc}",
                "on-click": "volumecontrol -o m",
                "on-click-right": "hyprctl dispatch exec pavucontrol",
                "on-scroll-up": "volumecontrol -o i",
                "on-scroll-down": "volumecontrol -o d",
                "min-length": 1,
                "max-length": 6
        },
        "hyprland/submap": {
                "format": "üÆã",
                "max-length": 8,
                "tooltip": false
        },
        "custom/recording": {
                "exec": "recording w",
                "return-type": "json",
                "format": "{}",
                "max-length": 8,
                "tooltip": false,
                "interval": 1
        },
        "custom/wifi": {
                "exec": "wifiinfo",
                "return-type": "json",
                "format": "{}",
                "tooltip": true,
                "on-click": "wifimenu -o i",
                "interval": 1,
                "min-length": 1,
                "max-length": 7
        },
        "network": {
                "format-wifi": " {essid} ",
                "format-ethernet": " {ifname} ",
                "tooltip": false,
                "on-click": "wifimenu -o i",
                "interval": 1,
                "min-length": 1,
                "max-length": 20
        },
        "cpu": {
                "format-icons": ["‚°Ä", "‚£Ä", "‚£Ñ", "‚£§", "‚£¶", "‚£∂", "‚£∑", "‚£ø"],
