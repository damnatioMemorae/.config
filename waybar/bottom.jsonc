{
        "mod": "dock",
        "name": "main",
        "layer": "top",
        "position": "bottom",
        "margin": "-10 20 10 20",
        "height": 36,
        "fixed-center": true,
        "exclusive": true,
        "passthrough": false,
        "gtk-layer-shell": false,
        "reload_style_on_change": true,
        "modules-left": [
                "custom/separators",
                "group/updates",
                // "custom/separators",
                "hyprland/workspaces",
                ""
        ],
        "modules-center": [
                "custom/separators",
                "backlight",
                "custom/separators",
                "pulseaudio",
                ""
        ],
        "modules-right": ["group/hardware", ""],
        "group/updates": {
                "orientation": "horizontal",
                "modules": [
                        "hyprland/language",
                        "hyprland/submap",
                        "custom/updates",
                        "custom/recording"
                ]
        },
        "group/hardware": {
                "orientation": "horizontal",
                // "drawer": {
                //         "transition-duration": 500,
                //         "children-class": "not-power",
                //         "transition-left-to-right": false
                // },
                "modules": [
                        // "custom/wifi",
                        "network",
                        "custom/separators",
                        // "cpu#icon",
                        "cpu",
                        "custom/separators",
                        // "memory#icon",
                        "memory",
                        "custom/separators",
                        // "temperature#icon",
                        "temperature",
                        "custom/separators",
                        // "custom/gpuinfo#icon",
                        "custom/gpuinfo",
                        "custom/separators",
                        // "battery#icon",
                        "battery",
                        "custom/separators"
                ]
        },
        "custom/updates": {
                "exec": "systemupdate t",
                "return-type": "json",
                "format": "{}",
                "rotate": 0,
                "interval": 43200,
                "tooltip": true,
                "signal": 20
        },
        "hyprland/workspaces": {
                "rotate": 0,
                "all-outputs": true,
                "active-only": false,
                "on-click": "activate",
                "disable-scroll": false,
                "on-scroll-down": "hyprctl dispatch workspace -1",
                "on-scroll-up": "hyprctl dispatch workspace +1",
