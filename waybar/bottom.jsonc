                        "26": []
                }
        },
        "hyprland/language": {
                "format": "{}",
                "format-en": "üÆã",
                "format-ru": "ü¨ã",
                // "format-en": "Ó≠î",
                // "format-ru": "Ó≠ë",
                "on-click": "keyboardswitch"
        },
        "cava": {
                "framerate": 120,
                "autosens": 1,
                "bars": 40,
                "method": "pulse",
                "source": "auto",
                "stereo": false,
                "reverse": false,
                "bar_delimiter": 0,
                "monstercat": false,
                "waves": true,
                "noise_reduction": 0.77,
                "input_delay": 0,
                "format-icons": ["‚£ø", "‚£æ", "‚£Ω", "‚£ª", "‚¢ø", "‚°ø", "‚£ü", "‚£ø"],
                "actions": {
                        "on-click-right": "mode"
                }
        },
        "backlight#icon": {
                "states": {
                        "min": 0,
                        "midl": 25,
                        "mid": 50,
                        "midh": 75,
                        "max": 100
                },
                "device": "intel_backlight",
                "format": "{icon}",
                "on-click": "$HOME/.local/share/bin/hyprsunset",
                "on-click-right": "shaders",
                "tooltip-format": "{icon} {percent}",
                "format-icons": ["Óéç", "Óèì", "Óèë", "Óèè", "Óèé", "Óèç", "Óèä", "Óèà", "Óéõ"],
                "on-scroll-up": "brightnessctl set 1%+",
                "on-scroll-down": "brightnessctl set 1%-",
                "min-length": 1
        },
        "backlight": {
                "device": "intel_backlight",
                "format": " {percent} ",
                // "on-click": "$HOME/.local/share/bin/hyprsunset",
                "on-click": "shaders night",
                "on-click-right": "shaders",
                "tooltip-format": "{icon} {percent}%",
                "on-scroll-up": "brightnessctl set 1%+",
                "on-scroll-down": "brightnessctl set 1%-",
                "min-length": 1
        },
        "backlight-slider": {
                "min": 0,
                "max": 100,
                "orientation": "horizontal"
        },
        "pulseaudio#icon": {
                "states": {
                        "min": 0,
                        "midl": 25,
                        "mid": 50,
                        "midh": 75,
                        "max": 100
                },
                "format": "{icon}",
                "format-muted": "Û∞ùü",
                "format-icons": {
                        "headphone": "Û∞ãã",
                        "hands-free": "Û∞•∞",
                        "headset": "Û∞•∞",
                        "default": ["Û∞ïø", "Û∞ñÄ", "Û∞ïæ"]
                },
                "tooltip": true,
                "tooltip-format": "Device: {desc}",
                "on-click": "volumecontrol -o m",
                "on-click-right": "hyprctl dispatch exec pavucontrol",
                "on-scroll-up": "volumecontrol -o i",
                "on-scroll-down": "volumecontrol -o d",
                "min-length": 1,
                "max-length": 6
        },
        "pulseaudio": {
                "format": " {volume} ",
                "tooltip": true,
                "tooltip-format": "Device: {desc}",
                "on-click": "volumecontrol -o m",
                "on-click-right": "hyprctl dispatch exec pavucontrol",
                "on-scroll-up": "volumecontrol -o i",
                "on-scroll-down": "volumecontrol -o d",
                "min-length": 2,
                "max-length": 6
        },
        "pulseaudio-slider": {
                "min": 0,
                "max": 100,
                "orientation": "horizontal"
        },
        "hyprland/submap": {
                "format": "üÆã",
                "max-length": 8,
                "tooltip": false
        },
        "custom/recording": {
                "exec": "recording w",
                "return-type": "json",
                "format": "{}",
                "max-length": 8,
                "tooltip": false,
                "interval": 1
        },
        "custom/wifi": {
                "exec": "wifiinfo",
                "return-type": "json",
                "format": "{}",
                "tooltip": true,
                "on-click": "wifimenu -o i",
                "interval": 1,
                "min-length": 1,
                "max-length": 7
        },
        "network": {
                "format-wifi": " {essid} ",
                "format-ethernet": " {ifname} ",
                "tooltip": false,
                "on-click": "wifimenu -o i",
                "interval": 1,
                "min-length": 1,
                "max-length": 20
        },
        "cpu#icon": {
                "states": {
                        "norm": 30,
                        "warning": 50,
                        "critical": 80
                },
                "format": "Ôíº",
                "on-click": "cpu",
                "interval": 1,
                "tooltip": false,
                "min-length": 1,
                "max-length": 7
        },
        "cpu": {
                "states": {
                        "norm": 30,
                        "warning": 50,
                        "critical": 80
                },
                "format": " {usage}% ",
                "on-click": "cpu",
                "tooltip": true,
                "interval": 1,
                "min-length": 1,
                "max-length": 6
        },

        //‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
        // CPU TEMPERATURE

        "temperature#icon": {
                "hwmon-path": "/sys/devices/pci0000:00/0000:00:18.3/hwmon/hwmon3/temp1_input",
                "critical-treshold": 40,
                "interval": 1,
                "format": "Ôãá"
        },
        "temperature": {
                "hwmon-path": "/sys/devices/pci0000:00/0000:00:18.3/hwmon/hwmon3/temp1_input",
                "critical-treshold": 10,
                "interval": 1,
                "format": " {temperatureC}¬∞C "
        },
        "custom/gpuinfo#icon": {
                "format": "Û∞¢Æ",
                "tooltip": false,
                "interval": 5,
                "min-length": 1,
                "max-length": 10,
                "menu": "on-click",
                "menu-file": "~/.config/waybar/menu/gpu.xml",
                "menu-actions": {
                        "Default": "gpu d",
                        "Overclock": "gpu o",
                        "Low": "gpu l"
                }
        },
        "custom/gpuinfo": {
                "exec": "gpuinfo",
                "return-type": "json",
                "format": " {} ",
                "tooltip": true,
                "interval": 5,
                "min-length": 4,
                "max-length": 8,
                "menu": "on-click",
                "menu-file": "~/.config/waybar/menu/gpu.xml",
                "menu-actions": {
                        "Default": "gpu d",
                        "Overclock": "gpu o",
                        "Low": "gpu l"
                }
        },
        "memory#icon": {
                "states": {
                        "norm": 30,
                        "warning": 50,
                        "critical": 80
                },
                "format": "Û∞òö",
                // "format": "ÓøÖ",
                "format-critical": "Û∞Ä¶",
                "tooltip": false,
                "tooltip-format": "Memory Used: {used:0.1f} GB / {total:0.1f} GB",
                "interval": 5,
                "min-length": 1,
                "max-length": 8
        },
        "memory": {
                "states": {
                        "norm": 30,
                        "warning": 50,
                        "critical": 80
                },
                "format": " {used} ",
                "format-critical": " {percentage}% ",
                "tooltip": true,
                "tooltip-format": "Memory Used: {used:0.1f} GB / {total:0.1f} GB",
                "interval": 5,
                "min-length": 1,
                "max-length": 7
        },
        "battery#icon": {
                "states": {
                        "full": 100,
                        "good": 99,
                        "warning": 50,
                        "critical": 15
                },
                "format": "{icon}",
                "format-icons": ["Û∞Åº", "Û∞ÅΩ", "Û∞Åæ", "Û∞Åø", "Û∞ÇÄ", "Û∞ÇÅ", "Û∞ÇÇ"],
                "format-full": "Û±Éå",
                "format-warning": "Û∞Åª",
                "format-critical": "Û±Éç",
                "format-charging": "Û∞ö•",
                "tooltip-format": "Time to Empty: {time}",
                "tooltip-format-charging": "Time to Full: {time}",
                "interval": 1,
                "min-length": 1,
                "max-length": 8
        },
        "battery": {
                "states": {
                        "full": 100,
                        "good": 99,
                        "warning": 50,
                        "critical": 15
                },
                "format": " {capacity} ",
                "format-full": " {capacity} ",
                "format-warning": " {capacity} ",
                "format-critical": " {capacity} ",
                "format-charging": " {capacity} ",
                "tooltip-format": "Time to Empty: {time}",
                "tooltip-format-charging": "Time to Full: {time}",
                "interval": 1,
                "min-length": 1,
                "max-length": 5
        },
        "custom/separator": {
                "format": "ÓÇ∂"
        },
        "custom/separators": {
                "format": " "
        }
}
