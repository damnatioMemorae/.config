                        "13": "Ó≠å",
                        "14": "Ó≠ç",
                        "15": "Ó≠é",
                        "16": "Ó≠è",
                        "17": "Ó≠ê",
                        "18": "Ó≠ë",
                        "19": "Ó≠í",
                        "20": "Ó≠ì",
                        "21": "Ó≠î",
                        "22": "Ó≠ï",
                        "23": "Ó≠ñ",
                        "24": "Ó≠ó",
                        "25": "Ó≠ò",
                        "26": "Ó≠ô",
